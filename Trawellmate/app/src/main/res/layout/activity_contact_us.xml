<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:orientation="vertical"
    tools:context=".ContactUsAct">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/ID_sts_bg_lay"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:background="@drawable/grad_theme"
            android:elevation="10dp" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_gravity="center"
                        android:layout_marginLeft="10dp"
                        android:padding="14dp"
                        android:src="@drawable/ic_arrow_back" />

                    <TextView
                        android:id="@+id/notify"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginLeft="10dp"
                        android:padding="10dp"
                        android:text=""
                        android:textSize="@dimen/text_size_large" />

                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="20dp"
                    android:layout_marginTop="10dp"
                    android:fontFamily="@font/open_san_script"
                    android:text="TraWellMate"
                    android:textColor="#444444"
                    android:textSize="@dimen/text_size_extra_large" />

                <View

                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginHorizontal="20dp"
                    android:layout_marginVertical="8dp"
                    android:background="@color/divider_color" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="25dp"
                    android:lineSpacingExtra="6dp"
                    android:text="World's leading tour and travels Booking website,Over 30,000 packages worldwide. Book travel packages and enjoy your holidays with distinctive experience" />

                <!-- //trawellmate-->

                <View

                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginHorizontal="20dp"
                    android:layout_marginVertical="5dp" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="25dp"
                    android:lineSpacingExtra="6dp"
                    android:text="Trawellmate Destination Management Pvt. Ltd.\nWe Provide Outsourced Software Development Services To Over 50 Clients From 21 Countries." />

                <!--            address-->
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="20dp"
                    android:layout_marginTop="25dp"
                    android:fontFamily="@font/open_san_script"
                    android:text="Address"
                    android:textColor="#444444"
                    android:textSize="@dimen/text_size_large" />

                <View

                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginHorizontal="20dp"
                    android:layout_marginVertical="8dp"
                    android:background="@color/divider_color" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="25dp"
                    android:lineSpacingExtra="6dp"
                    android:text="Santhosh Shopping Complex, Near Passport Seva Kendra,, Kannan Varkey Bridge, Alappuzha, Kerala, IN" />

                <!--            Contatct info-->
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="20dp"
                    android:layout_marginTop="25dp"
                    android:fontFamily="@font/open_san_script"
                    android:text="CONTACT INFO"
                    android:textColor="#444444"
                    android:textSize="@dimen/text_size_large" />

                <View

                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginHorizontal="20dp"
                    android:layout_marginVertical="8dp"
                    android:background="@color/divider_color" />


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="25dp"
                    android:lineSpacingExtra="6dp"
                    android:text="Toll Free: 914772967111\nLandline: 914772967111\nEmail: happiness@trawellmate.com" />

                <!--        follow with us-->
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="20dp"
                    android:layout_marginTop="25dp"
                    android:fontFamily="@font/open_san_script"
                    android:text="FOLLOW WITH US"
                    android:textColor="#444444"
                    android:textSize="@dimen/text_size_large" />

                <View

                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginHorizontal="20dp"
                    android:layout_marginVertical="8dp"
                    android:background="@color/divider_color" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="25dp"
                    android:lineSpacingExtra="6dp"
                    android:text="Join the thousands of other There are many variations of passages of Lorem Ipsum available" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="20dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_marginHorizontal="5dp"
                        android:background="@drawable/circle_white_bg"
                        android:padding="10dp"
                        android:src="@drawable/ic_facebook"
                        android:tint="#646363" />

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_marginHorizontal="5dp"
                        android:background="@drawable/circle_white_bg"
                        android:padding="10dp"
                        android:src="@drawable/ic_twitter"
                        android:tint="#646363" />

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_marginHorizontal="5dp"
                        android:background="@drawable/circle_white_bg"
                        android:padding="6dp"
                        android:src="@drawable/ic_youtube"
                        android:tint="#646363" />

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_marginHorizontal="5dp"
                        android:background="@drawable/circle_white_bg"
                        android:padding="8dp"
                        android:src="@drawable/ic_pinterest"
                        android:tint="#646363" />

                </LinearLayout>

            </LinearLayout>


        </RelativeLayout>

    </LinearLayout>

</androidx.coordinatorlayout.widget.CoordinatorLayout>